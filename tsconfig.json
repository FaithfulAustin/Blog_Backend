{
    "compilerOptions": {
      "experimentalDecorators": true,
      "target": "es2017",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strictPropertyInitialization": false,
      "strict": true, 
      "outDir": "./dist",
      "rootDir": "src",
      "noImplicitAny": true,
    },
    "include": [
      "src/**/*",
      "env.d.ts"
      ],
    "exclude": [
      "node_modules"
    ]
  }